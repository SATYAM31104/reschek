    "preview": "npm run build && npm run start",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build dist .react-router",
    "postinstall": "react-router typegen"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
