    "@react-router/serve": "^7.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "isbot": "^5.1.27",
    "pdfjs-dist": "^5.4.54",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-router": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
